<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Controller">
   <children>
      <Accordion layoutX="14.0" layoutY="14.0" />
      <Button defaultButton="true" layoutX="218.0" layoutY="14.0" mnemonicParsing="false" onAction="#buttonClick" prefHeight="25.0" prefWidth="94.0" text="knopochka" textFill="RED">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <TextField fx:id="textField" layoutX="14.0" layoutY="14.0" onKeyReleased="#checkText" prefHeight="25.0" prefWidth="204.0" />
      <Label fx:id="outputLabel" layoutX="14.0" layoutY="45.0" prefHeight="17.0" prefWidth="204.0" />
      <Button cancelButton="true" layoutX="218.0" layoutY="41.0" mnemonicParsing="false" onAction="#resetButton" prefHeight="25.0" prefWidth="94.0" text="resetButton">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="Button" disable="true" layoutX="14.0" layoutY="76.0" mnemonicParsing="false" onAction="#helloButton" prefHeight="25.0" prefWidth="299.0" text="Hello">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <TextArea fx:id="textArea" layoutX="16.0" layoutY="109.0" prefHeight="200.0" prefWidth="299.0" />
   </children>
</AnchorPane>
